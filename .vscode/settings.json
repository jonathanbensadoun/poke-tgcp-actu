{
    "workbench.colorCustomizations": {
        "activityBar.background": "#282F33",
        "titleBar.activeBackground": "#384147",
        "titleBar.activeForeground": "#F9FAFA"
    },
    "editor.tokenColorCustomizations": {
      "comments": "#2e613f",
      "textMateRules": []
    },
    "gradle.javaDebug.cleanOutput": true,
    
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[typescript]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[html]" : {
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.detectIndentation": false,
